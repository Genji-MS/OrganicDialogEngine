msgctxt "96"
msgid ""
"{FF05}{0F99}{FF19}{C7B7}Hitomi{C7B8}{FF16}Oh, {FF06}was that Tomoko?\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "97"
msgid "{FF05}{0F9A}It was, wasn't it?{FF00}"
msgstr ""

msgctxt "98"
msgid "That's right.{FF00}"
