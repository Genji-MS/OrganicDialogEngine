msgctxt "162"
msgid ""
"{FF05}{FFFF}{FF19}{C7B7}Student{C7B8}{FF16}Ahah... Ha...\n"
"heh hm heheheh...\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "163"
msgid ""
"{FF05}{FFFF}{FF19}{C7B7}Young Woman{C7B8}{FF16}This forum's supposed to\n"
