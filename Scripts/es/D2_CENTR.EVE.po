msgctxt "112"
msgid ""
"{FF05}{0812}{FF19}{C7B7}Yu-Ichi{C7B8}{FF16}Oh, welcome back, you guys.\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "113"
msgid ""
"{FF05}{0813}{FF19}{C7B7}Hitomi{C7B8}{FF16}Oh, Yu-Ichi.\n"
"You're back?\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "114"
msgid ""
"{FF05}{0814}{FF19}{C7B7}Yu-Ichi{C7B8}{FF16}Yeah. I'm studying how to\n"
"hack right now.\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "115"
msgid ""
"{FF05}{0815}{FF19}{C7B7}Nemissa{C7B8}{FF16}Hmmm, good for you.\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "116"
msgid ""
"{FF05}{0816}{FF19}{C7B7}Yu-Ichi{C7B8}{FF16}I want to become a full-fledged\n"
"hacker and get myself a cool handle name.\n"
"{FF03}{FF02}{FF05}{0817}Then I can work with all of you as\n"
"a member of Spookies. Man, I can't\n"
"wait.\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "117"
msgid ""
"{FF05}{0818}{FF19}{C7B7}Spooky{C7B8}{FF16}Oh, {FF80}.\n"
"Find out anything at the Astro Museum?\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "118"
msgid ""
