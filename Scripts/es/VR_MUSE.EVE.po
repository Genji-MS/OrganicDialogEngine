msgstr ""

msgctxt "77"
msgid ""
"{FF05}{1260}{FF19}{C7B7}Voice{C7B8}{FF16}Welcome... I shall help\n"
"you, drifter in this cyber world...\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "78"
msgid ""
"{FF05}{1261}{FF19}{C7B7}Nemissa{C7B8}{FF16}What is this place...?\n"
"Who are you?\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "79"
msgid ""
"{FF05}{1262}{FF19}{C7B7}Voice{C7B8}{FF16}......\n"
"{FF03}{FF02}{FF05}{1263}If you wish, I shall secure this\n"
"memory in the rifts of the cyber\n"
"world...\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "80"
msgid ""
"{FF05}{1264}{FF19}{C7B7}Nemissa{C7B8}{FF16}What's with this place...?\n"
"That voice is just repeating itself.\n"
"{FF03}{FF00}"
msgstr ""

msgctxt "81"
msgid ""
"{FF05}{1265}{FF19}{C7B7}Voice{C7B8}{FF16}Welcome... If you wish, I\n"
"shall secure this memory in the\n"
"rifts of the cyber world...\n"
"{FF03}{FF00}"
msgstr ""

