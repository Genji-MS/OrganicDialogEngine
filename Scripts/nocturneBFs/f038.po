msgctxt "type:0,entry:HASHIRA1,segment:0"
msgid ""
"<start>[7:FF07]> There is a pillar.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA2,segment:0"
msgid ""
"<start>[7:FF07]> Will you lower the pillar?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:HASHIRA_SEL,segment:0"
msgid "<start>Yes."
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER1,segment:0"
msgid ""
"<start>[7:FF07]> It's smooth.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER2,segment:0"
msgid ""
"<start>[7:FF07]> It looks hard.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER3,segment:0"
msgid ""
"<start>[7:FF07]> Main character\n"
"= debug!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER3,segment:1"
msgid ""
"<start>........\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER3,segment:2"
msgid ""
"<start>........\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER3,segment:3"
msgid ""
"<start>........\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA_ANSWER3,segment:4"
msgid ""
"<start>> Blah.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA3,segment:0"
msgid ""
"<start>[7:FF07]> The pillar has already been\n"
"lowered.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:HASHIRA4,segment:0"
msgid ""
"<start>[7:FF07]> All the pillars have been lowered.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN,segment:0"
msgid ""
"<start>[7:FF07]The shrine you can see from\n"
"the entrance is His Lordship's\n"
"resting place.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN,segment:1"
msgid ""
"<start>But, the pillars holding it up are\n"
"guarded by the four Devas, and\n"
"they're ultra-powerful.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN,segment:2"
msgid ""
"<start>Don't go near them!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN2,segment:0"
msgid ""
"<start>[7:FF07]I can't believe it!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN2,segment:1"
msgid ""
"<start>You actually beat all four of\n"
"the Devas!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:GAIAN2,segment:2"
msgid ""
"<start>M-Master!! Make me your student!\n"
"<end>"
msgstr ""

#. Passerby
msgctxt "type:0,entry:CHUUOU,segment:0"
msgid ""
"<start>[7:FF07]Right above here!\n"
"<end>"
msgstr ""

#. Passerby
msgctxt "type:0,entry:CHUUOU,segment:1"
msgid ""
"<start>His Lordship is up there!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:BOSS,segment:0"
msgid ""
"<start>[7:FF07]> Will you open the door?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:BOSS_SEL,segment:0"
msgid "<start>Yes."
msgstr ""

#. Voice
msgctxt "type:0,entry:BOSS2,segment:0"
msgid ""
"<start>[7:FF07]Who doth awaken me from\n"
"my slumber?\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:BOSS2_2,segment:0"
msgid ""
"<start>[7:FF07]............\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:BOSS2_2,segment:1"
msgid ""
"<start>...So, thou art the demon who came\n"
"from afar...?\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:BOSS2_2,segment:2"
msgid ""
"<start>I am [2:FF02]Masakado Taira[2:FF01],\n"
"the guardian of Tokyo for\n"
"the last thousand years.\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS2_3,segment:0"
msgid ""
"<start>[7:FF07]............\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS2_3,segment:1"
msgid ""
"<start>...Thou art [12:FF01],\n"
"art thou not?\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS2_3,segment:2"
msgid ""
"<start>The king of chaos, who hath gained\n"
"the powers of demons despite being\n"
"human...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS2_4,segment:0"
msgid ""
"<start>[7:FF07][12:FF01]...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS2_4,segment:1"
msgid ""
"<start>Why hast thou come this far to\n"
"see me?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:BOSS_SEL3,segment:0"
msgid "<start>Lend me your power."
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3,segment:0"
msgid ""
"<start>[7:FF07]So, thou hast come for mine\n"
"assistance...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3,segment:1"
msgid ""
"<start>............\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3,segment:2"
msgid ""
"<start>But, thy Tokyo is a Tokyo my light\n"
"cannot reach.\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3,segment:3"
msgid ""
"<start>I cannot set foot in thy world...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3_2,segment:0"
msgid ""
"<start>[7:FF07]Thou art direct and to the point...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3_3,segment:0"
msgid ""
"<start>[7:FF07]I see... Thou hast defeated\n"
"the four Devas out of sport...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS3_3,segment:1"
msgid ""
"<start>Outstanding...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS4,segment:0"
msgid ""
"<start>[7:FF07]Let me ask thee a question...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS4,segment:1"
msgid ""
"<start>Wilt thou bring peace to Tokyo?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:BOSS5,segment:0"
msgid ""
"<start>[7:FF07]> The [2:FF02]Lord's Sword[2:FF01] began shining\n"
"even brighter!\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS6,segment:0"
msgid ""
"<start>[7:FF07]I see...\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:BOSS_SEL2,segment:0"
msgid "<start>Yes."
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS7,segment:0"
msgid ""
"<start>[7:FF07]Let me ask thee a question...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS7,segment:1"
msgid ""
"<start>Wilt thou bring peace to Tokyo?\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS8,segment:0"
msgid ""
"<start>[7:FF07]Then, I shall lend thee my power.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:BOSS9,segment:0"
msgid ""
"<start>[7:FF07]> You obtained the Magatama\n"
"[2:FF03]Masakados[2:FF01].\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS10,segment:0"
msgid ""
"<start>[7:FF07]............\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS10,segment:1"
msgid ""
"<start>Farewell.\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS10,segment:2"
msgid ""
"<start>Be it a new world, it is still Tokyo...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS10,segment:3"
msgid ""
"<start>Thou art the new guardian of\n"
"the land...\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS11,segment:0"
msgid ""
"<start>[7:FF07]I look forward to thy world,\n"
"conqueror. May you prove\n"
"victorious in your war for peace.\n"
"<end>"
msgstr ""

#. Masakado
msgctxt "type:0,entry:BOSS11,segment:1"
msgid ""
"<start>Now go, [12:FF01]!\n"
"<end>"
msgstr ""

