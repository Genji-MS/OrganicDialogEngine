msgctxt "type:0,entry:msg_exit,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]Come back anytime.\n"
"I'll try my best to find \n"
"more items!\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:msg_sel01,segment:0"
msgid "<start>Yes."
msgstr ""

msgctxt "type:0,entry:msg_ticket,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]Thank you for buying so much.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_ticket,segment:1"
msgid ""
"<start>Take this [2:FF05]Lucky Ticket[2:FF01] \n"
"if you'd like.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_ticket,segment:2"
msgid ""
"<start>Acquire ten of them for a raffle. \n"
"You can also use them in any of \n"
"the Junk Shops.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_ticket_item,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]Ah, you found many \n"
"[2:FF05]Lucky Tickets[2:FF01]!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_ticket_item,segment:1"
msgid ""
"<start>Okay, pick one box that you like.\n"
"I'll give you what's inside.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_ticket_item2,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]Uh... It's a [2:FF05][3:FF04][2:FF01].\n"
"...Is it something useful?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:msg_sel02,segment:0"
msgid "<start>Big box."
msgstr ""

msgctxt "type:0,entry:msg_roki_item,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]By the way... I got this \n"
"from a demon for 60 Macca the\n"
"other day.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_item,segment:1"
msgid ""
"<start>It's called the [2:FF02]Star Key[2:FF01].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_item,segment:2"
msgid ""
"<start>But, I'm not interested in it.\n"
"I'm only interested in man-made\n"
"items.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_item,segment:3"
msgid ""
"<start>So, I'll give it to you \n"
"if you'd like.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_item,segment:4"
msgid ""
"<start>Do you want it?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:msg_roki_sel,segment:0"
msgid "<start>Yes."
msgstr ""

msgctxt "type:0,entry:msg_roki_yes,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]I see.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_yes,segment:1"
msgid ""
"<start>Okay then, here you go.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_yes,segment:2"
msgid ""
"<start>> [13:FF01] received\n"
"the [2:FF02]Star Key[2:FF01].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_yes,segment:3"
msgid ""
"<start>Well, I hope you enjoy\n"
"the rest of your shopping. \n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_no,segment:0"
msgid ""
"<start>[7:FF07][6:FF01]I see.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_no,segment:1"
msgid ""
"<start>Well, tell me when you want it.\n"
"I'll give it to you anytime.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_roki_no,segment:2"
msgid ""
"<start>Yeah, just like that.\n"
"<end>"
msgstr ""
