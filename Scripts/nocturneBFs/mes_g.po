msgid ""
"<start>[7:FF07][6:FF01]I don't just collect junk.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_talk_asak_2_hearts_a,segment:1"
msgid ""
"<start>For example, I sell Magatama as\n"
"well.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_talk_asak_2_hearts_a,segment:2"
msgid ""
"<start>I thought they would be useful to\n"
"you... It took me awhile to find them.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_talk_asak_2_hearts_a,segment:3"
msgid ""
"<start>I have no clue how to use these\n"
"things, but...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:msg_talk_asak_2_hearts_a,segment:4"
msgid ""
"<start>Feel free to buy them, if you'd like.\n"
