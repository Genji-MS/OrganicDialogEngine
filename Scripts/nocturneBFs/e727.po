#. Blond child
msgctxt "type:0,entry:MSG_001,segment:0"
msgid ""
"<start>[7:FF05]Well done. You've proven yourself\n"
"with your awesome power.\n"
""
msgstr ""

#. Old gentleman in a wheelchair
msgctxt "type:0,entry:MSG_002,segment:0"
msgid ""
"<start>[7:FF05]All of you who have lurked in\n"
"darkness and waited faithfully\n"
"for this day, hear me...\n"
""
msgstr ""

#. Old gentleman in a wheelchair
msgctxt "type:0,entry:MSG_002B,segment:0"
msgid ""
"<start>[7:FF05]A new demon of darkness has been\n"
"born.\n"
""
msgstr ""

#. Old gentleman in a wheelchair
msgctxt "type:0,entry:MSG_003,segment:0"
msgid ""
"<start>[7:FF05]...The time has come.\n"
""
msgstr ""

msgctxt "type:0,entry:MSG_004,segment:0"
msgid ""
"<start>[7:FF05]Together, let us march onward!\n"
""
msgstr ""

msgctxt "type:0,entry:MSG_005,segment:0"
msgid ""
"<start>[7:FF07]To the final battle...\n"
"where our true enemy awaits!\n"
""
msgstr ""

msgctxt "type:0,entry:MSG_YHVH01,segment:0"
msgid ""
"<start>[7:FF07]...You have created a new demon in\n"
"your heart's likeness, fallen angel?\n"
""
msgstr ""

msgctxt "type:0,entry:MSG_YHVH02,segment:0"
msgid ""
"<start>[7:FF07]Then, doom shall set us apart...\n"
""
msgstr ""

msgctxt "type:0,entry:MSG_YHVH03,segment:0"
msgid ""
"<start>[7:FF07]now and forevermore...\n"
""
msgstr ""
