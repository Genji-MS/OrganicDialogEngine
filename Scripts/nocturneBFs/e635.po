#. [[
msgctxt "type:0,entry:MSG_001,segment:0"
msgid ""
"<start>[7:FF07]...Hi, [12:FF01].\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_002,segment:0"
msgid ""
"<start>[18:][7:FF07]I heard about the trial by combat,\n"
"and I thought it might be you\n"
"who was the winner.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_003,segment:0"
msgid ""
"<start>[7:FF07][18:]Getting here was tough,\n"
"to say the least...\n"
"but I'm glad I made it.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_004,segment:0"
msgid ""
"<start>[7:FF07][18:][12:FF01]...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_004,segment:1"
msgid ""
"<start>[18:]There's something I want to tell you.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_005,segment:0"
msgid ""
"<start>[7:FF07][18:]I decided to play by the rules\n"
"of this world.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_006,segment:0"
msgid ""
"<start>[7:FF07][18:]You know what the purpose of\n"
"this new world is, right?\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_007,segment:0"
msgid ""
"<start>[7:FF07][18:]I... I want to try my hand\n"
"at the creation.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_008,segment:0"
msgid ""
"<start>[7:FF07][18:]...You probably think I'm crazy.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_008_a,segment:0"
msgid ""
"<start>[7:FF07][18:]But, I remembered that voice...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_008_a,segment:1"
msgid ""
"<start>[18:]...that voice I heard when\n"
"the world changed...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_008_b,segment:0"
msgid ""
"<start>[7:FF07][18:]You must've heard it too,\n"
"[12:FF01],\n"
"since you survived.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009,segment:0"
msgid ""
"<start>[7:FF07][18:]...After we went our separate ways,\n"
"I started thinking.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009,segment:1"
msgid ""
"<start>[18:]Not just about what I have to do\n"
"in this world,\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009,segment:2"
msgid ""
"<start>[18:]but why the world ended up like this.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009,segment:3"
msgid ""
"<start>[18:]And then, I realized...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009,segment:4"
msgid ""
"<start>[18:]...that there were too many\n"
"unnecessary things occupying\n"
"the previous world.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009B,segment:0"
msgid ""
"<start>[7:FF07][18:]There were lots of things,\n"
"and lots of people...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009B,segment:1"
msgid ""
"<start>[18:]But, nothing new was being made.\n"
"Time passed, and nothing changed.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_009B,segment:2"
msgid ""
"<start>[18:]What the world really needed...\n"
"it was not getting.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_010,segment:0"
msgid ""
"<start>[7:FF07][18:]...It's true that I lost everything.\n"
"It was the saddest thing imaginable.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_010,segment:1"
msgid ""
"<start>[18:]But, I survived the Conception.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_010,segment:2"
msgid ""
"<start>[18:]I was chosen...\n"
"That's what I'm going to believe,\n"
"and now, I have to look forward.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011,segment:0"
msgid ""
"<start>[7:FF07][18:]I still feel a great sadness\n"
"in my heart...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011,segment:1"
msgid ""
"<start>[18:]but if I can swallow it down...\n"
"endless possibilities await...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011,segment:2"
msgid ""
"<start>[18:]The power of creation...\n"
"will be mine.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:0"
msgid ""
"<start>[7:FF07][18:]\"Choose,\" the voice said to me.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:1"
msgid ""
"<start>[18:]The world doesn't need any more\n"
"worthless things...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:2"
msgid ""
"<start>[18:]That's why I want to create\n"
"my ideal society,\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:3"
msgid ""
"<start>[18:]a paradise ruled by the strong,\n"
"the chosen...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:4"
msgid ""
"<start>[18:]The world of Yosuga.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_012,segment:5"
msgid ""
"<start>[18:][12:FF01],\n"
"you've fought your way this far.\n"
"You know what I'm saying,\n"
"don't you?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:MSG_011_2,segment:0"
msgid "<start>Yeah, I get it."
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_3,segment:0"
msgid ""
"<start>[7:FF07][18:]...I'm happy to hear that.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_3,segment:1"
msgid ""
"<start>[18:]I knew you'd understand,\n"
"[12:FF01].\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_3,segment:2"
msgid ""
"<start>[18:]I might ask you to help me out,\n"
"then.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_4,segment:0"
msgid ""
"<start>[7:FF07]......\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_4,segment:1"
msgid ""
"<start>[18:]...That's too bad.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_4,segment:2"
msgid ""
"<start>[18:]I was hoping you'd understand.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_4,segment:3"
msgid ""
"<start>[18:]But, no matter what you say,\n"
"I won't change my mind.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_5,segment:0"
msgid ""
"<start>[7:FF07][18:]...I see.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_5,segment:1"
msgid ""
"<start>[18:]But I believe in you,\n"
"[12:FF01].\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_011_5,segment:2"
msgid ""
"<start>[18:]I believe that one day\n"
"you'll come to accept it.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:0"
msgid ""
"<start>[7:FF07][18:]I'm going to start collecting\n"
"Magatsuhi.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:1"
msgid ""
"<start>[18:]Honestly, I don't know where\n"
"to begin...\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:2"
msgid ""
"<start>[18:]But, since Yosuga is the Reason\n"
"of the strong, I'll see what\n"
"I can do on my own first.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:3"
msgid ""
"<start>[18:]...I'm glad we had the chance\n"
"to talk today.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:4"
msgid ""
"<start>[18:]We both survived the Conception...\n"
"I'm sure we'll cross paths again.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:MSG_013,segment:5"
msgid ""
"<start>[18:]See you later, [12:FF01].\n"
"<end>"
msgstr ""
