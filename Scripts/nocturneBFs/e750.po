#. Noble voice
msgctxt "type:0,entry:MSG_001,segment:0"
msgid ""
"<start>[7:FF07][12:FF01]...\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_001,segment:1"
msgid ""
"<start>I can see by the darkness in your\n"
"soul that you have chosen to\n"
"disregard my warning...\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_002,segment:0"
msgid ""
"<start>[7:FF07]...You are beyond the point\n"
"of redemption.\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_002,segment:1"
msgid ""
"<start>I cannot turn a blind eye to\n"
"the fallen angel's abominable plan...\n"
"to foster the birth of a new demon.\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_003,segment:0"
msgid ""
"<start>[7:FF07]Delivering the candelabra to\n"
"the Labyrinth of Amala is just\n"
"as grave a sin.\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_003,segment:1"
msgid ""
"<start>Listen, Fiend! Foul demon whose\n"
"soul is tainted with darkness!\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_004,segment:0"
msgid ""
"<start>[7:FF07]Listen! And tremble in fear!\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_004,segment:1"
msgid ""
"<start>I am Metatron! I am one with god!\n"
"<end>"
msgstr ""

#. Noble voice
msgctxt "type:0,entry:MSG_004,segment:2"
msgid ""
"<start>...By his will, I shall destroy you!!\n"
"<end>"
msgstr ""

#. Metatron
msgctxt "type:0,entry:MSG_005,segment:0"
msgid ""
"<start>[7:FF07]Feel the wrath of god,\n"
"disciple of darkness!!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MSG_003B,segment:0"
msgid ""
"<start>[7:FF07]> You obtained\n"
"the [2:FF04]Candelabrum of Wisdom[2:FF01].\n"
"<end>"
msgstr ""
