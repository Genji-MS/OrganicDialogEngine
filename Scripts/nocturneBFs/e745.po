msgid ""
"<start>[7:FF07]......\n"
"<end>"
msgstr ""

#. Black Rider
msgctxt "type:0,entry:MSG_001,segment:1"
msgid ""
"<start>We exist... to watch o'er\n"
"the candelabra...\n"
"<end>"
msgstr ""

#. Black Rider
msgctxt "type:0,entry:MSG_002,segment:0"
msgid ""
"<start>[7:FF07]As long as thou seekest\n"
"the candelabra...\n"
"thou art mine enemy...\n"
"<end>"
msgstr ""

#. Black Rider
msgctxt "type:0,entry:MSG_002,segment:1"
msgid ""
"<start>I... the Black Rider...\n"
"shall judge thee...\n"
"<end>"
msgstr ""

#. Black Rider
msgctxt "type:0,entry:MSG_002B,segment:0"
msgid ""
"<start>[7:FF07]Thy death draweth nigh,\n"
"o sinful one...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MSG_003,segment:0"
msgid ""
"<start>[7:FF07]> You obtained\n"
"the [2:FF02]Candelabrum of Wisdom[2:FF01].\n"
"<end>"
msgstr ""
