msgctxt "type:0,entry:MSG_004_DANTE_01,segment:0"
msgid ""
"<start>[7:FF07]> It seems as if someone\n"
"is watching you...\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_004_DANTE_02,segment:0"
msgid ""
"<start>[7:FF07]Hey kid... Didn't I tell you\n"
"to lay low?\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_004_DANTE_02,segment:1"
msgid ""
"<start>Playtime's over. Now...\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_004_DANTE_02,segment:2"
msgid ""
"<start>Let's rock!\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_004_DANTE_03,segment:0"
msgid ""
"<start>[7:FF07]...Don't disappoint me.\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_01,segment:0"
msgid ""
"<start>[7:FF07]...A game's no fun if you\n"
"don't know the rules.\n"
"So, here's the deal...\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_01,segment:1"
msgid ""
"<start>To get past here,\n"
"you need to turn on\n"
"all the [2:FF02]switches[2:FF01] first.\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_01,segment:2"
msgid ""
"<start>Alright, I'm gonna count to three.\n"
"That's how long you've got\n"
"to come up with a plan.\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_02,segment:0"
msgid ""
"<start>[7:FF07]One...\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_03,segment:0"
msgid ""
"<start>[7:FF07]Two...\n"
"<end>"
msgstr ""

#. Dante's voice
msgctxt "type:0,entry:MSG_005_DANTE_04,segment:0"
msgid ""
"<start>[7:FF07]...Three!\n"
"<end>"
msgstr ""

