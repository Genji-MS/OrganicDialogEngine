#. Soul
msgctxt "type:0,entry:F016_SINEN_01_01,segment:2"
msgid ""
"<start>That's another bad thing that's\n"
"gonna happen to me...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN_01_02,segment:0"
msgid ""
"<start>[7:FF07]............\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN_01_02,segment:1"
