msgid "<start>Oh, yeah."
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_01YES,segment:0"
msgid ""
"<start>[7:FF07]...Man, she's hot!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_01NO,segment:0"
msgid ""
"<start>[7:FF07]Then, hurry up and go to\n"
"the Fountain of Life up ahead.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_01NO,segment:1"
msgid ""
"<start>...She's hot!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_02,segment:0"
msgid ""
"<start>[7:FF07]The Lady of the Fount is hot as\n"
"usual! You visiting?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_02YES,segment:0"
msgid ""
"<start>[7:FF07]You know... She's so hot!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_SINEN02_02NO,segment:0"
msgid ""
"<start>[7:FF07]Then, how've you been recovering!?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_INKYU01_01,segment:0"
msgid ""
"<start>[7:FF07]............\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_INKYU01_01,segment:1"
msgid ""
"<start>The hot one's always been\n"
"the Lady of the Fount...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:F016_INKYU01_02,segment:0"
msgid ""
"<start>[7:FF07]I'm telling ya,\n"
"the Lady of the Fount's\n"
"the hottest of them all!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_01,segment:0"
msgid ""
"<start>[7:FF07]The construction site over there is\n"
"now the home of the fairies.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_01,segment:1"
msgid ""
"<start>They locked the door, so we can't\n"
"get in...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_02,segment:0"
msgid ""
"<start>[7:FF07]But, I wonder if you can get in from\n"
"the east side?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_02,segment:1"
msgid ""
"<start>You should be able to get there by\n"
"taking the [2:FF02]Asakusa Tunnel[2:FF01].\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_02,segment:2"
msgid ""
"<start>Good thing Tokyo's circular.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_03,segment:0"
msgid ""
"<start>[7:FF07]...That's what I was going to say,\n"
"but you came from inside!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_03,segment:1"
msgid ""
"<start>So, what's in there?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_04,segment:0"
msgid ""
"<start>[7:FF07]The construction site's pretty quiet\n"
"now.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_05,segment:0"
msgid ""
"<start>[7:FF07]I heard there's a beautiful woman\n"
"somewhere around this side of\n"
"the park.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN03_05,segment:1"
msgid ""
"<start>...I prefer the cute little Pixies,\n"
"though.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_01,segment:0"
msgid ""
"<start>[7:FF07]You know about that [2:FF02]human girl[2:FF01] in\n"
"Shibuya?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_01,segment:1"
msgid ""
"<start>Seems that she came from some\n"
"hospital.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_02,segment:0"
msgid ""
"<start>[7:FF07]I heard that they were doing some\n"
"pretty cruel things at Shinjuku\n"
"Medical Center.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_02,segment:1"
msgid ""
"<start>Gaea cult, was it?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_03,segment:0"
msgid ""
"<start>[7:FF07]I......\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_03,segment:1"
msgid ""
"<start>............\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F016_SINEN04_03,segment:2"
msgid ""
"<start>...Prefer [12:FF02].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F016_GIMIC03_01,segment:0"
msgid ""
"<start>[7:FF07]> The door is locked.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F016_GIMIC03_02,segment:0"
msgid ""
"<start>[7:FF07]> You used the [2:FF02]key to Yoyogi Park[2:FF01].\n"
"<end>"
