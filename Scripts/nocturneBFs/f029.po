msgid ""
"<start>[7:FF07]> There is no response from\n"
"the Manikin.\n"
"<end>"
msgstr ""

#. Male Manikin
msgctxt "type:0,entry:F029_MANE01,segment:0"
msgid ""
"<start>[7:FF07]J-J-Just awhile back, Sakahagi...\n"
"went to Yoyogi...\n"
"<end>"
msgstr ""

#. Male Manikin
msgctxt "type:0,entry:F029_MANE01,segment:1"
msgid ""
"<start>Th-Th-Thank goodness it was\n"
"dark...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN01_01,segment:0"
msgid ""
"<start>[7:FF07]A vicious-looking Manikin passed\n"
"by a short while back.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN01_01,segment:1"
msgid ""
"<start>After that, a female human passed\n"
"by.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN01_01,segment:2"
msgid ""
"<start>It's usually quiet here, but...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN01_02,segment:0"
msgid ""
"<start>[7:FF07]This is a rather quiet place,\n"
"compared to Tokyo in general.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN02_01,segment:0"
msgid ""
"<start>[7:FF07]Yoyogi Park is the place that\n"
"all the fairies are guarding.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN02_01,segment:1"
msgid ""
"<start>That vicious Manikin won't be able\n"
"to do anything just by himself.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F029_SINEN02_02,segment:0"
msgid ""
"<start>[7:FF07]That lady who was heading to\n"
"Yoyogi Park was lookin' fiiiiiine!\n"
"<end>"
msgstr ""
