#. [[
msgctxt "type:0,entry:ISAMU_ENT01A_01,segment:0"
msgid ""
"<start>[7:FF07]Geez, you're finally here.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01A_02,segment:0"
msgid ""
"<start>[7:FF07]You really are a nosy guy!\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01A_02,segment:1"
msgid ""
"<start>Well, it doesn't matter to me\n"
"anymore what you're thinking.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01A_02,segment:2"
msgid ""
"<start>But, if you're for Musubi, I've got\n"
"the perfect job for you.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01A_03,segment:0"
msgid ""
"<start>[7:FF07]Come through the door.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02A_01,segment:0"
msgid ""
"<start>[7:FF07]This is the forgotten temple that I\n"
"found while checking out\n"
"the network.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02A_01,segment:1"
msgid ""
"<start>There's a crapload of Magatsuhi\n"
"stored here.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02A_01,segment:2"
msgid ""
"<start>But, it's being closely guarded by\n"
"three demons.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02A_02,segment:0"
msgid ""
"<start>[7:FF07]Can you go and beat them? It\n"
"makes it easier on me.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01B_01,segment:0"
msgid ""
"<start>[7:FF07]...So, you actually came. You really\n"
"are a nosy guy.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01B_02,segment:0"
msgid ""
"<start>[7:FF07]Oh, well... Since you came this far,\n"
"I'm not going to stop you.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01B_02,segment:1"
msgid ""
"<start>If you want to come, then come.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT01B_03,segment:0"
msgid ""
"<start>[7:FF07]Come through the door.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_01,segment:0"
msgid ""
"<start>[7:FF07]This is the forgotten temple that I\n"
"found while checking out\n"
"the network.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_01,segment:1"
msgid ""
"<start>There's a crapload of Magatsuhi\n"
"stored here.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_01,segment:2"
msgid ""
"<start>But, it's being closely guarded by\n"
"three demons.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_02,segment:0"
msgid ""
"<start>[7:FF07]So... If you want to save that man,\n"
"then you'll have to beat them.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_02,segment:1"
msgid ""
"<start>If you can do that, I'll let you inside\n"
"the central temple.\n"
"<end>"
msgstr ""

#. [[
msgctxt "type:0,entry:ISAMU_ENT02B_02,segment:2"
msgid ""
"<start>Good luck!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:CENTER_DONE,segment:0"
msgid ""
