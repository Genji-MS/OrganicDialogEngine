msgctxt "type:0,entry:SINJIKU,segment:0"
msgid ""
"<start>[7:FF07]> Shinjuku Station...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:SINJIKU,segment:1"
msgid ""
"<start>> It's as quiet as a tomb.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:SINJIKU2,segment:0"
msgid ""
"<start>[7:FF07]> Nothing is showing on\n"
"the giant screen...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO,segment:0"
msgid ""
"<start>[7:FF07]You don't know where you are?\n"
"...You're joking, right?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO,segment:1"
msgid ""
"<start>Tokyo, of course!\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO,segment:2"
msgid ""
"<start>All of the humans are gone, though.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO2,segment:0"
msgid ""
"<start>[7:FF07]Hmmm... I just passed through\n"
"Shinjuku, so...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO2,segment:1"
msgid ""
"<start>If I go south, I should reach\n"
"Yoyogi and Shibuya.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO3,segment:0"
msgid ""
"<start>[7:FF07]I just saw a girl heading toward\n"
"Shibuya.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO3,segment:1"
msgid ""
"<start>I guess she really was a human...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO3,segment:2"
msgid ""
"<start>What? You're a human, too!?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO3,segment:3"
msgid ""
"<start>Or... are you a demon?\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO4,segment:0"
msgid ""
"<start>[7:FF07]*squint* I can see Aoyama over there...\n"
"I think.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO4,segment:1"
msgid ""
"<start>Looks like it's safe.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO4,segment:2"
msgid ""
"<start>But, you can't get there from here.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:KOKOTOKYO4,segment:3"
msgid ""
"<start>I already tried going from\n"
"Yoyogi Park, and that didn't work...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:EXITWARP,segment:0"
msgid ""
"<start>[7:FF07]> Will you leave the area?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EXITWARP_SEL,segment:0"
msgid "<start>Yes."
msgstr ""

