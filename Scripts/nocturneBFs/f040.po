msgctxt "type:0,entry:DOROP_START,segment:0"
msgid ""
"<start>[7:FF07]> You landed in an unexpected\n"
"location.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001B,segment:0"
msgid ""
"<start>[7:FF07]First, please return the [2:FF02]Candelabrum\n"
"of Foundation[2:FF01] to its rightful place.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001B,segment:1"
msgid ""
"<start>I will open the door leading to\n"
"where the candelabrum must be\n"
"returned.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001C,segment:0"
msgid ""
"<start>[7:FF07]...You can now proceed.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001C,segment:1"
msgid ""
"<start>I must warn you, though,\n"
"that powerful demons roam\n"
"the area ahead.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001C,segment:2"
msgid ""
"<start>Please be careful...\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001,segment:0"
msgid ""
"<start>[7:FF07]If you go through the door behind\n"
"you, you will find a pedestal.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001,segment:1"
msgid ""
"<start>Once you retrieve the candelabrum,\n"
"please set it on that pedestal.\n"
"<end>"
msgstr ""

#. Voice of the lady in black
msgctxt "type:0,entry:MSG_001,segment:2"
msgid ""
"<start>...I know you will be successful.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MSG_002,segment:0"
