msgstr ""

#. Izanami
msgctxt "type:0,entry:F045_AREA29_IZA003,segment:0"
msgid ""
"<start>[7:FF07]Unable to wield our strength,\n"
"<end>"
msgstr ""

#. Izanami
msgctxt "type:0,entry:F045_AREA29_IZA003,segment:1"
msgid ""
"<start>we awaited the one to whom\n"
"we could entrust the future.\n"
"<end>"
msgstr ""

#. Izanami
msgctxt "type:0,entry:F045_AREA29_IZA003_02,segment:0"
msgid ""
"<start>[7:FF07]...And now you have come.\n"
"<end>"
msgstr ""

#. Izanagi
msgctxt "type:0,entry:F045_AREA29_IZA004,segment:0"
msgid ""
"<start>[7:FF07][13:FF01], please accept\n"
"our gift.\n"
"<end>"
msgstr ""

#. Izanagi
msgctxt "type:0,entry:F045_AREA29_IZA004,segment:1"
msgid ""
"<start>It will help you create a new world\n"
"in this space filled with chaos,\n"
"<end>"
msgstr ""

#. Izanagi
msgctxt "type:0,entry:F045_AREA29_IZA004,segment:2"
msgid ""
"<start>as Izanami and I did when we met\n"
"long ago...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA29_IZA005,segment:0"
msgid ""
"<start>[7:FF07]> Your body surges with power...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA29_IZA005_002,segment:0"
msgid ""
"<start>[7:FF07]> You are now able to attack\n"
"more often.\n"
"<end>"
msgstr ""

#. Izanami
msgctxt "type:0,entry:F045_AREA29_IZA006,segment:0"
msgid ""
"<start>[7:FF07]Farewell, [13:FF01].\n"
"<end>"
msgstr ""

#. Izanami
msgctxt "type:0,entry:F045_AREA29_IZA006,segment:1"
msgid ""
"<start>Our job is done.\n"
"We pray for your success.\n"
"<end>"
msgstr ""

#. Izanagi and Izanami
msgctxt "type:0,entry:F045_AREA29_IZA007,segment:0"
msgid ""
"<start>[7:FF07]May the heavens bless you...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA30_ROSINSI00,segment:0"
msgid ""
"<start>[7:FF07]> You find yourself in\n"
"a strange place.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA30_ROSINSI01,segment:0"
msgid ""
"<start>[7:FF07]> A voice echoes in your head...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI02,segment:0"
msgid ""
"<start>[7:FF07]...Come...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI02,segment:1"
msgid ""
"<start>...Show me... the strength...\n"
"of a demon...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA30_ROSINSI03,segment:0"
msgid ""
"<start>[7:FF07]> The old man and the woman\n"
"disappeared...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI04,segment:0"
msgid ""
"<start>[7:FF07]...This way...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI05,segment:0"
msgid ""
"<start>[7:FF07]...So, you made it...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI06,segment:0"
msgid ""
"<start>[7:FF07]...Your strength... is outstanding...\n"
"<end>"
msgstr ""

#. Voice
msgctxt "type:0,entry:F045_AREA30_ROSINSI07,segment:0"
msgid ""
"<start>[7:FF07]...We shall meet again... soon...\n"
"<end>"
msgstr ""

#. Dr. Dark
msgctxt "type:0,entry:F045_AREA30_KUROJAKU,segment:0"
msgid ""
"<start>[7:FF07]By my master's orders,\n"
"I'll treat your wounds.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:F045_AREA30_KAIFUKU02,segment:0"
