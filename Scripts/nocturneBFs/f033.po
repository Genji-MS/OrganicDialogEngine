msgctxt "type:0,entry:KANMON02,segment:3"
msgid ""
"<start>[12:FF01], dost thou accept\n"
"that thou art a demon?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:KANMON_SEL,segment:0"
msgid "<start>Of course I'm a demon."
msgstr ""

#. Mithra
msgctxt "type:0,entry:KANMON03,segment:0"
msgid ""
"<start>[7:FF07]Then [12:FF01], dost thou\n"
"accept that thy heart is demonic?\n"
"<end>"
