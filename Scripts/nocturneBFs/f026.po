msgid ""
"<start>............\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F026_SINEN06_YES01,segment:3"
msgid ""
"<start>...Very well.\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F026_SINEN06_YES01,segment:4"
