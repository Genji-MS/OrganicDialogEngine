msgid ""
"<start>[7:FF07]I see...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:F026_SINEN07,segment:0"
msgid ""
"<start>[7:FF07]My friend told me that he ran into\n"
