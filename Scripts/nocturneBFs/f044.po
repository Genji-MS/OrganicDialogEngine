"<start>[7:FF07]Go ahead, have some more...\n"
"<end>"
msgstr ""

#. Soul
msgctxt "type:0,entry:BOTTAKURI_YES_04,segment:0"
msgid ""
"<start>[7:FF07]Are you feeling better?\n"
"Here's some more...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:BOTTAKURI_YES_05,segment:0"
