msgctxt "type:0,entry:MANE_B_03_2ND,segment:0"
msgid ""
"<start>[7:FF07]Ah! You're back!\n"
"<end>"
msgstr ""

#. Manikin on the right
msgctxt "type:0,entry:MANE_B_03_2ND,segment:1"
msgid ""
"<start>Are you here to tell him to hurry up\n"
"and summon Mara???\n"
"<end>"
msgstr ""

#. Manikin on the left
msgctxt "type:0,entry:MANE_B_03_YES,segment:0"
msgid ""
"<start>[7:FF07]See?! He thinks so too!\n"
"<end>"
msgstr ""

#. Manikin on the left
msgctxt "type:0,entry:MANE_B_03_YES,segment:1"
msgid ""
"<start>Now, hurry up and summon Mara!\n"
"<end>"
msgstr ""

#. Baphomet
msgctxt "type:0,entry:BAFO_02,segment:0"
