msgctxt "type:0,entry:m_tiisai01_02,segment:1"
msgid ""
"<start>> Go inside?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_tiisai01_02s,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_tiisai02,segment:0"
