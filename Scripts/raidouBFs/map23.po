msgctxt "type:0,entry:map23_ep07_scene04_02,segment:0"
msgid ""
"<start>[7:0005,00FF]> The curse on Raidou was broken.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:map23_ep07_scene04_03,segment:0"
msgid ""
"<start>[7:0005,00FF]> A noble voice rang out.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:map23_ep07_scene04_04,segment:0"
msgid ""
"<start>[7:0005,00FF]I see you have destroyed the source of\n"
