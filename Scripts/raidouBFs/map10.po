msgctxt "type:0,entry:HOSEKI,segment:1"
msgid ""
"<start>> The contents inside shift and warp.\n"
"Take the contents?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:HOSEKI_SEL,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:HOSEKI_NOZOKI,segment:0"
