msgctxt "type:0,entry:m_msg_hiruko_d_07,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] checked out the\n"
"Hiruko.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_07,segment:1"
msgid ""
"<start>> It judged that a strong, hard\n"
"fist would do the trick.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_08,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a gathering of Hiruko.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_09,segment:0"
msgid ""
"<start>[7:0005,00FF]> Hiruko have congregated here.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_10,segment:0"
msgid ""
"<start>[7:0005,00FF]> Hiruko are stacked on top of\n"
"each other.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_11,segment:0"
msgid ""
"<start>[7:0005,00FF]> Hiruko are blocking the way.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_msg_hiruko_d_12,segment:0"
msgid ""
"<start>[7:0005,00FF]> The Hiruko have been successfully\n"
"dispersed.\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_hiruko_02,segment:0"
