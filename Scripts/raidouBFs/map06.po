#. [[[[[[[[[[[[[[
msgctxt "type:0,entry:m_mes_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> There is nothing here.\n"
"<end>"
msgstr ""

#. [[[[[[[[[[[[[[
msgctxt "type:0,entry:m_mes_01,segment:1"
msgid ""
"<start>> Leave?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_YESNO,segment:0"
msgid "<start>Yes"
msgstr ""

#. [[[[[[[[[[[[[[
msgctxt "type:0,entry:m_mes_01_yes,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou left.\n"
"<end>"
msgstr ""

#. [[[[[[[[[[[[[[
msgctxt "type:0,entry:m_mes_01_no,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided to stay.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a gigantic tree.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi2,segment:0"
msgid ""
"<start>[7:0005,00FF]> There are bugs crawling all over\n"
"the tree.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi2_NAKAMA,segment:0"
msgid ""
"<start>[7:0005,00FF]> There are bugs crawling all over\n"
"the tree.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi2_NAKAMA,segment:1"
msgid ""
"<start>> [14:0000,0000] popped a bug into its\n"
"mouth.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi2_NAKAMA,segment:2"
msgid ""
"<start>> ......\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi2_NAKAMA,segment:3"
msgid ""
"<start>> [14:0000,0000] spit out the terrible-\n"
"tasting bug.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_NAKAMA,segment:0"
msgid ""
"<start>[7:0005,00FF]You are Raidou the 14th's pet\n"
"familiar, are you not...\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_NAKAMA,segment:1"
msgid ""
"<start>It seems the 14th's devil-summoning\n"
"prowess is quite advanced.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_1,segment:0"
msgid ""
"<start>[7:0005,00FF]You are a hard worker, Raidou the\n"
"14th.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_1,segment:1"
msgid ""
"<start>Would you like to enter the\n"
"Training Hall?\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_YES,segment:0"
msgid ""
"<start>[7:0005,00FF]Unfortunately, the Training Hall is\n"
"closed to you at this time.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_YES,segment:1"
msgid ""
"<start>Once it is open, you may use this\n"
"entrance whenever you wish.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_YES,segment:2"
msgid ""
"<start>Train and advance to your heart's\n"
"content.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_NO,segment:0"
msgid ""
"<start>[7:0005,00FF]I see.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_NAKAMA,segment:0"
msgid ""
"<start>[7:0005,00FF]Hey, you're the demon hanging out\n"
"with the 14th.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_NAKAMA,segment:1"
msgid ""
"<start>Take good care of Raidou, okay?\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_1,segment:0"
msgid ""
"<start>[7:0005,00FF]Raidou the 14th, you're doing great.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_1,segment:1"
msgid ""
"<start>Got any questions for me?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_KITUNE_R_R_01,segment:0"
msgid "<start>Ask about the Training Hall"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:0"
msgid ""
"<start>[7:0005,00FF]I getcha, I getcha. Well, don't\n"
"get careless.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:1"
msgid ""
"<start>You don't know how they're going\n"
"to come at you.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:2"
msgid ""
"<start>Especially in the Dark Realm...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:3"
msgid ""
"<start>So, the two of us on either side of\n"
"the shrine are here to help.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:4"
msgid ""
"<start>Don't hesitate to ask us anything\n"
"you need to know.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_YES,segment:5"
msgid ""
"<start>We're here for you, Raidou.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE_R_NO,segment:0"
msgid ""
"<start>[7:0005,00FF]> The fox made a melancholy yip.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_NO2,segment:0"
msgid ""
"<start>[7:0005,00FF]I see...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_NO2,segment:1"
msgid ""
"<start>Well, if you ask me, it's a good\n"
"idea to exercise at the Training\n"
"Hall.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_NO2,segment:2"
msgid ""
"<start>If you feel up for it, talk to the\n"
"guy on the other side, and he'll\n"
"take you to the Training Hall.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a washstand.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_1,segment:1"
msgid ""
"<start>> What will you do?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_TYOUZU,segment:0"
msgid "<start>Wash your hands"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_2,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou felt refreshed.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_3,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou felt cleaner.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_NAKAMA,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a washstand.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_NAKAMA,segment:1"
msgid ""
"<start>> [14:0000,0000] jumped in to take a\n"
"bath!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_NAKAMA,segment:2"
msgid ""
"<start>> ......\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_NAKAMA,segment:3"
msgid ""
"<start>> [14:0000,0000] looks contented.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a deep washstand. You can't see\n"
"the bottom...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV02,segment:0"
msgid ""
"<start>[7:0005,00FF]> What should Raidou do?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_TYOUZU_LV,segment:0"
msgid "<start>Enter"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV_YES,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou has been sucked into the\n"
"darkness...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV_NO,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou left.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV03,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a disused washstand.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV03,segment:1"
msgid ""
"<start>> The water in the basin is almost\n"
"completely dried up.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_TYOUZU_LV04,segment:0"
msgid ""
"<start>[7:0005,00FF]> The water has been drained from\n"
"the basin, revealing the bottomless\n"
"darkness inside...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Bugs might be attracted to\n"
"sweet material on the tree...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_2,segment:0"
msgid ""
"<start>[7:0005,00FF]> What should Raidou do?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_MUSI_A,segment:0"
msgid "<start>Spread the ice cream"
msgstr ""

msgctxt "type:1,entry:m_MUSI_B,segment:0"
msgid "<start>Spread the ice cream"
msgstr ""

msgctxt "type:1,entry:m_MUSI_C,segment:0"
msgid "<start>Spread the syrup"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_3,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou spread the ice cream onto\n"
"the large tree and waited.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_4,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou spread the syrup onto the\n"
"large tree and waited.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_no,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou did nothing.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_5,segment:0"
msgid ""
"<start>[7:0005,00FF]> The tree is partially covered with\n"
"ice cream.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_6,segment:0"
msgid ""
"<start>[7:0005,00FF]> The tree is partially covered with\n"
"syrup.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou checked the gigantic tree.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET1,segment:1"
msgid ""
"<start>[809E]......\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET1,segment:2"
msgid ""
"<start>> It's crawling with ants!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET2,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou checked the gigantic tree.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET2,segment:1"
msgid ""
"<start>> ......\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET2,segment:2"
msgid ""
"<start>> A rare-looking beetle is sucking\n"
"on the syrup.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_musi1_GET2,segment:3"
msgid ""
"<start>> Raidou obtained the [2:0005,00FF]Beetle[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE__R_KOUBUTU,segment:0"
msgid ""
"<start>[7:0005,00FF]I'm fine, but it's nice of you to\n"
"ask.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE__R_KOUBUTU,segment:1"
msgid ""
"<start>I'll gladly take one when the time\n"
"comes.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE_ABURA0,segment:0"
msgid ""
"<start>[7:0005,00FF]> The Inari statues stand as if\n"
"they were staring at you.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_DAME,segment:0"
msgid ""
"<start>[7:0005,00FF]How's it going, Raidou Kuzunoha\n"
"the 14th?\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_DAME,segment:1"
msgid ""
"<start>Tell you what. If you get better\n"
"at the job...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_DAME,segment:2"
msgid ""
"<start>I'll let you in on a little something.\n"
"[2:0004,00FF]Come back later[2:0001,00FF], okay, Mr. 14th?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE_ABURA1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Offer a fried tofu?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE_ABURA_YES,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou split the fried tofu in half,\n"
"and gave each statue a piece.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_KITUNE_ABURA_NO,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to offer the\n"
"fried tofu.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2,segment:0"
msgid ""
"<start>[7:0005,00FF]Whoa! Thanks for the fried tofu,\n"
"Mr. 14th.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_1,segment:0"
msgid ""
"<start>[7:0005,00FF]To pay you back...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_1,segment:1"
msgid ""
"<start>I'll let you in on something.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_2,segment:0"
msgid ""
"<start>[7:0005,00FF]There're other Training Halls\n"
"around the Capital.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_2,segment:1"
msgid ""
"<start>Like this one, they're good places\n"
"to train yourself and your demons...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_2,segment:2"
msgid ""
"<start>But, there's a secret to 'em.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_2,segment:3"
msgid ""
"<start>Every Training Hall has a special\n"
"item that'll pump you up.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_ABURA2_2,segment:4"
msgid ""
"<start>If you bring in a sharp-eyed demon,\n"
"you should be able to spot 'em.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:0"
msgid ""
"<start>[7:0005,00FF]Mr. 14th, you're doing great.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:1"
msgid ""
"<start>I'll give you a reward...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:2"
msgid ""
"<start>An invitation to a members-only\n"
"club made just for guys like\n"
"you...\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:3"
msgid ""
"<start>The [2:0002,00FF]Training Hall[2:0001,00FF]!\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:4"
msgid ""
"<start>It's the time-tested training ground\n"
"for the Kuzunoha clan.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:5"
msgid ""
"<start>There are lots of demons around, so\n"
"you can find just about any demon\n"
"you're looking for.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN,segment:6"
msgid ""
"<start>If you want to check it out, go to\n"
"the washstand out in front.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN_R,segment:0"
msgid ""
"<start>[7:0005,00FF]The Training Hall is where the\n"
"Kuzunoha clan goes to shape up.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN_R,segment:1"
msgid ""
"<start>There are lots of demons around, so\n"
"you can find just about any demon\n"
"you're looking for.\n"
"<end>"
msgstr ""

#. Fox (Right)
msgctxt "type:0,entry:m_mes_KITUNE_R_SYUGEN_R,segment:2"
msgid ""
"<start>If you want to check it out, go to\n"
"the washstand out in front.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:0"
msgid ""
"<start>[7:0005,00FF]You are a hard worker, Raidou the\n"
"14th.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:1"
msgid ""
"<start>I shall bestow upon you a reward...\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:2"
msgid ""
"<start>Not material possessions, but\n"
"beneficial information that will\n"
"aid you in your journey.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:3"
msgid ""
"<start>Know you the Training Hall?\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:4"
msgid ""
"<start>It is the ancient ground where\n"
"members of the Kuzunoha clan\n"
"cultivate their strength.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:5"
msgid ""
"<start>Demons abound within its walls,\n"
"and if you seek a particular demon,\n"
"you may well find it there.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_SYUGEN,segment:6"
msgid ""
"<start>If you wish to enter the Training\n"
"Hall, examine the washstand in\n"
"front of the building.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_DAME,segment:0"
msgid ""
"<start>[7:0005,00FF]You are a hard worker, Raidou the\n"
"14th.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_DAME,segment:1"
msgid ""
"<start>We Inari statues support the\n"
"Kuzunoha clan.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_DAME,segment:2"
msgid ""
"<start>We are fragments of the Ukanomitama\n"
"spirit.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_DAME,segment:3"
msgid ""
"<start>Should you grow in strength as a\n"
"Kuzunoha...\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_DAME,segment:4"
msgid ""
"<start>I shall grant you access to the\n"
"[2:0004,00FF]Training Hall[2:0001,00FF]. Until then, continue\n"
"to train vigorously.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_ABURA2,segment:0"
msgid ""
"<start>[7:0005,00FF]I am honored that you have brought\n"
"me fried tofu, Raidou the 14th.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_ABURA2_1,segment:0"
msgid ""
"<start>[7:0005,00FF]To show my gratitude...\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_ABURA2_1,segment:1"
msgid ""
"<start>I shall impart helpful information.\n"
"<end>"
msgstr ""

#. Fox (Left)
msgctxt "type:0,entry:m_mes_KITUNE_L_ABURA2_2,segment:0"
