msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI01_SCENE08_10,segment:0"
msgid ""
"<start>[7:0005,00FF]There are dangers there foreign\n"
"to this mortal world.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI01_SCENE08_11,segment:0"
msgid ""
"<start>[7:0005,00FF]It is a dwelling place of demons.\n"
"Be careful, for your life is in\n"
"great danger there.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:EPI01_SCENE08_12,segment:0"
msgid ""
"<start>[7:0005,00FF]> The Ritual of Entry begins, and\n"
"Raidou is sent to [2:0002,00FF]Dark Tsukudo-Cho[2:0001,00FF]...\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou Kuzunoha the 14th,\n"
"I've awaited your return.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Please, come this way.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Admiral, Kuzunoha has arrived.\n"
"[23:]<end>"
msgstr ""

#. Man
msgctxt "type:0,entry:EPI07_SCENE02_04,segment:0"
msgid ""
"<start>[7:0005,00FF]Ah, yes... Raidou Kuzunoha,\n"
"the guardian of our Capital.\n"
"<end>"
msgstr ""

#. Man
msgctxt "type:0,entry:EPI07_SCENE02_05,segment:0"
msgid ""
"<start>[7:0005,00FF]I'm depending on you.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou, this man is an important\n"
"naval officer.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]He's... crucial to the future\n"
"of our country.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Unfortunately, he was [2:0002,00FF]cursed[2:0001,00FF]\n"
"and is now unable to move.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...I would ask for your help\n"
"in saving this man.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I will perform a Purification Ritual.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The Admiral was cursed by an\n"
"evil thought cast upon him.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]If we exorcise and purify that\n"
"thought, the effects should\n"
"subside.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]But, we cannot ignore the origin\n"
"of this curse, Raidou.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]It will be your job to locate the one\n"
"responsible and smite him.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_15,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]In order to do so, I would ask you\n"
"to become a [2:0002,00FF]sacrifice[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI07_SCENE02_16,segment:0"
msgid ""
"<start>[7:0005,00FF]...What!? But, Raidou's the one\n"
"breaking the curse!\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_17,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]That is precisely why we cannot\n"
"transfer it onto a doll.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_18,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall accept the curse placed upon\n"
"the Admiral as your own burden.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_19,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]In doing so, you will gain the ability to\n"
"follow those evil thoughts back to\n"
"their origin.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_20,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Will you do this, Raidou?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI07_SCENE02_SEL,segment:0"
msgid "<start>I accept."
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_21,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You embody the qualities of\n"
"a true Devil Summoner.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_22,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]So be it. I will begin the Ritual of\n"
"Purification.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_23,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou, please prepare for\n"
"the ritual.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_24,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The curse is attacking the Admiral.\n"
"If you break concentration for even\n"
"a moment, it will tear you apart.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_25,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Use the utmost care!\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_26,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I have completed the ritual.\n"
"The Admiral is safe.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_27,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Now, it is imperative that you find\n"
"and destroy the source of this curse.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_28,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Concentrate... Feel the curse.\n"
"Your body should know what\n"
"course to take.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:EPI07_SCENE02_29,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou sought the curse and\n"
"his body melted into thin air.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_30,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I'm afraid you do not have the option of\n"
"refusal, Raidou.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_31,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]However, I understand the peril\n"
"involved in the ritual.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_32,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I can grant you time to prepare,\n"
"if you so desire.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_33,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Please return once you have\n"
"adequately prepared for the task\n"
"at hand.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_34,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The ritual will transfer the curse\n"
"to a replacement body called the\n"
"Fuchikoma--a sacrifice.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_35,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The curse binding the Admiral\n"
"was placed by a demon. You will\n"
"need to take his place.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_36,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Doing so will allow you to follow\n"
"the curse back to its origin.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_37,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Will you accept this, Raidou?[23:]\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_SCENE02_38,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...Have you steeled yourself?[23:]\n"
"<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]You did it, Raidou. The Admiral is safe.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I'm glad I have the chance to\n"
"thank you personally.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Still, using demons to curse\n"
"the Admiral? What a terrifying man.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Major General Munakata, I mean.\n"
"I'm sure you figured it out.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]This is because of that Super Soldier\n"
"Project of his...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]This all happened because he\n"
"considered the Navy an obstacle.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]We're safe now, Raidou, but we cannot\n"
"let Munakata get away with this.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I would like to ask for your\n"
"cooperation in stopping Munakata.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]The Navy has been using spies,\n"
"myself included, to monitor his\n"
"activities.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]We have reason to believe that if\n"
"his Super Soldier Project continues,\n"
"the Capital will be in danger.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I've been granted permission to\n"
"share information, if there's\n"
"anything you need to know.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Once you understand the scope of\n"
"his project, I believe you'll agree\n"
"with our assessment.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]What would you like to know?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI07_SCENE05_SEL01,segment:0"
msgid "<start>Ask about Munakata"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]He's considered a genius in the Army,\n"
"and rightly so.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_15,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]His flawless strategic decisions\n"
"earned him the reputation of\n"
"being able to see the future.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_16,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]But lately, his activities have been...\n"
"suspicious, to say the least.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_17,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Building a ship underground...\n"
"The Special Guard... Demons...\n"
"He's connected to them all.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_18,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]...Any other questions?[23:]\n"
"<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_19,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Ah, the Super Soldier Project...\n"
"I was shocked when I heard about it.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_20,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]It's Munakata's plan to beef up\n"
"defenses and expand the country,\n"
"but the details... It's creepy.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_21,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Japan needs an edge over the\n"
"other world powers, since we lack\n"
"abundant natural resources...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_22,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]...Munakata's plan is to create\n"
"an invincible army of soldiers\n"
"who cannot die.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_23,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]You've seen them around;\n"
"they're the Special Guards in red.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_24,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]They're corpses underneath\n"
"those uniforms.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_25,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Hence, they do not fear death,\n"
"and will keep fighting until nothing\n"
"is left--literally.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_27,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I would imagine the ship being\n"
"built underground is to transport\n"
"this army overseas.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_28,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]He needs an unsinkable ship to\n"
"transport an unstoppable army.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_29,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]This is something we simply cannot\n"
"allow to happen.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_30,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Raising the dead to fight a war?\n"
"It's disgusting!\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_31,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]There are lines that shouldn't be\n"
"crossed, and Munakata crossed\n"
"a big one.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_32,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]If this project is seen through,\n"
"he'll become the most powerful man\n"
"in the country.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_33,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]We have to do something before\n"
"that can happen.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_34,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]The Red Cape phenomenon is\n"
"caused by those radio towers\n"
"the Army constructed.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_35,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Waves from the towers affect\n"
"certain people... causing them\n"
"to break down.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_36,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Those people become a kind of\n"
"walking dead... From their minds,\n"
"the Hiruko are born.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_37,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]This is all just another facet of\n"
"the Super Soldier Project.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_38,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]...Hopefully now you can comprehend\n"
"the horror Munakata is trying to\n"
"unleash with his Super Soldiers.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_39,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]We would like to continue\n"
"monitoring the Army in hopes\n"
"of stopping Munakata.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_40,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]But, his demons and that undead\n"
"army make it difficult for a normal\n"
"human being to get involved.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_41,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]You, on the other hand, are a\n"
"Devil Summoner. I had hoped you\n"
"might lend us your services.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_42,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]His army... The underground ship...\n"
"Those radio towers...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_43,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]...and that Daidouji girl you're\n"
"looking for...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_44,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]The Super Soldier Project is\n"
"the thread tying them together.\n"
"And, we have to stop it.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_45,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]The key lies in [2:0004,00FF]Sakuradayama[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_46,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]There's a tower on the mountain\n"
"called [2:0002,00FF]Waden One[2:0001,00FF]. It's the first\n"
"of the Army's radio towers.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_47,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Sources say Munakata visits that site\n"
"often, even now.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_48,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]If you can get in there...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_49,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]We may be able to find out\n"
"what he's planning to do next.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_50,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Raidou, can I entrust you\n"
"with this mission?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI07_SCENE05_SEL02,segment:0"
msgid "<start>Yes."
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_51,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Thank you, Raidou.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_52,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Remember, you're up against a man\n"
"who controls the dead. It's dangerous,\n"
"but I trust you.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_53,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I'll make the necessary arrangements\n"
"with your superior.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_54,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]...Good luck, Raidou Kuzunoha.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_55,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF].........\n"
"Please, Raidou... Think it over.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SCENE05_56,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]This mission could very well\n"
"determine the future of Japan.[23:]\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Hm... What magic is this, that\n"
"there are two Raido Kuzunoha\n"
"the 14ths?\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_SCENE04_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]...I sensed a strange presence\n"
"and this man appeared.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_SCENE04_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]Goto didn't think he was a demon,\n"
"so we brought him here.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...I see. However, his spiritual essence...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I understand.\n"
"You are a lost soul.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You were not meant for this\n"
"time and place. You are a foreigner;\n"
"an anomaly.\n"
"[23:]<end>"
msgstr ""

#. Goto
msgctxt "type:0,entry:EPI09_SCENE04_07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0017,00FF,0006,00FF]Anomaly? Are we going to have to\n"
"get rid of them?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]No, this person is the Raido\n"
"Kuzunoha the 14th of another\n"
"space and time.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...That's the case, is it not?\n"
"Are you Raido Kuzunoha the 14th?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI09_SCENE04_SEL01,segment:0"
msgid "<start>Yes."
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]As I thought.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...However, it is detrimental to\n"
"this world for you to remain here.\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI09_SCENE04_12,segment:0"
msgid ""
"<start>[7:0005,00FF]...That isn't funny, [12:0001,00FF].\n"
"Tell her that you're Raidou.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Thus, you must return to the space\n"
"and time from whence you came.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]To this end, I will permit you\n"
"use of the [2:0002,00FF]Amatsu Kanagi[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI09_SCENE04_15,segment:0"
msgid ""
"<start>[7:0005,00FF]...What's the Amatsu Kanagi?\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_16,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]It is a sacred artifact.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_17,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]By using its power in a ritual,\n"
"you may be able to return safely\n"
"to your world of origin.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_18,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...However, before that can happen,\n"
"I do require some effort on your part.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_19,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The Amatsu Kanagi has been sealed\n"
"to prevent it falling into evil hands.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_20,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Earth, flame, water... It was\n"
"separated into several prisms,\n"
"each with a guardian.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_21,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raido from another world...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_22,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]If you wish to set things right,\n"
"you must earn three of these prisms.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_23,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Once you have overcome the\n"
"guardians and obtained them, I\n"
"will show you how to return home.\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI09_SCENE04_24,segment:0"
msgid ""
"<start>[7:0005,00FF]...We've had stranger jobs.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI09_SCENE04_25,segment:0"
msgid ""
"<start>[7:0005,00FF]Just like Yatagarasu said,\n"
"[12:0001,00FF]. Let's find those\n"
"prisms.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_26,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]If you are prepared, then choose\n"
"a destination.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_27,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Each prism has been hidden\n"
"in a different location.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_28,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Decide where you will begin.[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI09_SCENE04_SEL02,segment:0"
msgid "<start>Dark Sakuradayama"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_29,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]So be it. I shall send you to\n"
"Dark Sakuradayama. Godspeed.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_30,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]So be it. I shall send you to\n"
"Dark Ginza-Cho. Godspeed.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_31,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]So be it. I shall send you to\n"
"Dark Harumi-Cho. Godspeed.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE04_32,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]When you believe yourself to be\n"
"ready, return here.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_KARASU_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You are ready to seek the prisms,\n"
"are you not?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_KARASU_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Deleted!\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_KARASU_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]In that case, decide which location\n"
"you are to search.[23:]\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You have obtained the three prisms\n"
"of earth, water, and flame.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]As promised, I shall grant you\n"
"the Amatsu Kanagi and send you\n"
"back to your proper world.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Relinquish the prisms unto me...\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:EPI09_SCENE08_04,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou handed the three prisms\n"
"to the Herald of Yatagarasu.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Very well. When combined with\n"
"the Heaven Prism which I protect...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...the prisms assume their true form\n"
"as the Amatsu Kanagi.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:EPI09_SCENE08_07,segment:0"
msgid ""
"<start>[7:0005,00FF]> The four prisms overlapped,\n"
"forming a single object.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:EPI09_SCENE08_08,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained the [2:0005,00FF]Amatsu Kanagi[2:0001,00FF].\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Come forward, Raido.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_SCENE08_10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]...Yes?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Accompany this man to the\n"
"Ushigome-gaeri Bridge.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]That is the boundary between our\n"
"world and the Dark Realm, the most\n"
"ideal location for the ritual.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Use the Ritual of Soul-Sending,\n"
"the technique used to send evil spirits\n"
"back whence they came.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI09_SCENE08_14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Go forth, Raido, and he of another\n"
"space and time.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_RAIDO_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]I'd be lying if I said I was eager to\n"
"cross swords with myself.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_RAIDO_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]I'm all too aware of what Raido\n"
"the 14th can do...\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_RAIDO_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]You couldn't have expected this,\n"
"but you got your bearings quickly\n"
"and carried out your duty.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_RAIDO_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]I admire that. You're certainly\n"
"worthy to be called Raido the 14th.\n"
"[23:]<end>"
msgstr ""

#. Raido
msgctxt "type:0,entry:EPI09_RAIDO_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0016,00FF,0006,00FF]...Hmm, is that too much like\n"
"self-praise? Hmhm...\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:CROW01_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> There is a bell inside the shrine.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:CROW01_02,segment:0"
msgid ""
"<start>[7:0005,00FF]> Ring the bell?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:CROW_SEL01,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:CROW01_03,segment:0"
msgid ""
"<start>[7:0005,00FF]> The bell resounded through\n"
"the area...\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I am the Herald of Yatagarasu, the\n"
"one who vouchsafes the peace of\n"
"the Capital.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou the 14th, Devil Summoner of\n"
"Kuzunoha, is something amiss?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:CROW_SEL02,segment:0"
msgid "<start>Perform the Ritual of Entry."
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Very well. I shall send you to the\n"
"Dark Realm of the Capital.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You do not require anything?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Then, I shall be on my way back\n"
"to Yatagarasu. A place not on\n"
"this plane...\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_06,segment:0"
msgid ""
"<start>[7:0005,00FF]> The Herald of Yatagarasu\n"
"disappeared.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]So, Raidou the 14th, it seems you\n"
"have grown in strength since\n"
"we met.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Initiate[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_KAGE,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Promising Youth[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Taisho Swordsman[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Local Hero[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Urban Mystic[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Prodigy[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Worthy Summoner[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Gallant Hero[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Avatar of Spirits[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Rising Star[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Renowned Summoner[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Master of Demons[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Morning Star[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Demons' Terror[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK15,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Vigilant Guardian[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_08_LANK16,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You shall henceforth be known as\n"
"Raidou the 14th, [2:0002,00FF]Timeless Summoner[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_09,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou's title has changed!\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You may find these of use now.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_11,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained 2 Tubes.\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_11_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You may also find this to be valuable...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_11_02,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Please put it to good use during\n"
"your mission.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_11_03,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained the [2:0005,00FF]Mystic Eye[2:0001,00FF].\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_11_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]This mirror will reveal the abilities\n"
"of hostile demons you face in battle.\n"
"I'm sure it will prove useful.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_11_04,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...That is all.\n"
"We are counting on you.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I look forward to your growth in\n"
"your efforts to safeguard the\n"
"Capital's peace.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:CROW02_13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou the 14th, is there anything\n"
"else you require of me?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_14,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained 4 Tubes.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:CROW02_15,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained 6 Tubes.\n"
"<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]You look like you have something\n"
"to say.[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI07_SADA_SEL,segment:0"
msgid "<start>...You tricked me."
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_02,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]That is simply how events have\n"
"played out. I won't ask you to\n"
"let bygones be bygones...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_03,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]But, understand that we aren't\n"
"the enemy. I swear it upon this\n"
"crest.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Do you mean Mr. Narumi? He\n"
"looks worse than he is; there's\n"
"no internal damage.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]He put up quite a fight. He was\n"
"able to dodge several of our\n"
"best men.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Our pride was hurt worse than\n"
"his body...\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]Under urgent circumstances such\n"
"as these, we believe this is the\n"
"best possible solution.\n"
"[23:]<end>"
msgstr ""

#. Sadakichi
msgctxt "type:0,entry:EPI07_SADA_08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]I'm sorry, but we aren't giving you\n"
"a choice. Assuming you wish your\n"
"friend to be unharmed, that is.\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu01,segment:0"
msgid ""
"<start>[7:0005,00FF]This is important, [12:0001,00FF],\n"
"so remember it.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu02,segment:0"
msgid ""
"<start>[7:0005,00FF]Most people don't pay much attention\n"
"to this beat-up old shrine...\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu02,segment:1"
msgid ""
"<start>But, it's actually of great importance\n"
"to the Kuzunoha clan.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu03,segment:0"
msgid ""
"<start>[7:0005,00FF]This is where we contact our client\n"
"in the Capital.\n"
"<end>"
msgstr ""

#. Mysterious woman
msgctxt "type:0,entry:EPI00_karasu04,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I was told that Raidou Kuzunoha\n"
"the 14th would be arriving soon.\n"
"[23:]<end>"
msgstr ""

#. Mysterious woman
msgctxt "type:0,entry:EPI00_karasu05,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You are that Devil Summoner,\n"
"I presume?\n"
"[23:]<end>"
msgstr ""

#. Mysterious woman
msgctxt "type:0,entry:EPI00_karasu06,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I am the [2:0002,00FF]Herald[2:0001,00FF] who brings tidings\n"
"from [2:0002,00FF]Yatagarasu[2:0001,00FF].\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu06_TUIKA001,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF][2:0001,00FF]I am the one who ordered the\n"
"Kuzunoha clan to maintain peace\n"
"in our capital city.\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu07,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]So, long story short, you want\n"
"Raidou here to keep watch over\n"
"the capital city.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu08,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Yes, the city is currently unstable\n"
"because so many people have\n"
"gathered there.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu09,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The boundary separating this world\n"
"from the Dark Realm is weak.\n"
"[2:0002,00FF]Demons[2:0001,00FF] will start to appear.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu10,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]There will be serious ramifications\n"
"if they should attack residents of\n"
"the city.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu11,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]In order to prevent that from\n"
"happening, I am calling on you,\n"
"Raidou Kuzunoha the 14th.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu12,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Demons live in the [2:0002,00FF]Dark Realm[2:0001,00FF], which\n"
"normal humans are forbidden to enter.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu13,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You, however, are permitted as\n"
"a Devil Summoner.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu14,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You must subdue the demons before\n"
"they can cause any public harm.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu15,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Such is the will of Yatagarasu.\n"
"Do you understand?[23:]\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:EPI00_KARASU_SEL01,segment:0"
msgid "<start>I will protect the people."
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu15_YES,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I am glad to hear you say that.\n"
"...There is one last thing.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu15_NO,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You may not understand now,\n"
"but you will once you arrive in\n"
"the Capital.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu15_NO,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]One last thing...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu16,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You will have an assistant while\n"
"in the Capital...\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu16,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]...A man named [2:0002,00FF]Narumi[2:0001,00FF].\n"
"He runs a detective agency--on his\n"
"own, at the moment.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu16,segment:2"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]You are to become employed at\n"
"the [2:0002,00FF]Narumi Detective Agency[2:0001,00FF] and\n"
"work as a detective.\n"
"[23:]<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:EPI00_karasu17,segment:0"
msgid ""
"<start>[7:0005,00FF][22:001E,00FF,0006,00FF]A detective agency, huh? That's\n"
"a pretty swanky way to gather\n"
"information, isn't it?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu18,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I have already made the proper\n"
"arrangements with Mr. Narumi.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI00_karasu18,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The fate of the Capital rests on\n"
"your shoulders, Raidou Kuzunoha.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_TUIKA_NOROI_001,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Raidou Kuzunoha the 14th.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_TUIKA_NOROI_001,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Why have you not yet broken\n"
"the curse?\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_TUIKA_NOROI_002,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Your duty is to defeat the demon\n"
"who placed the curse, and save\n"
"the Admiral.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:EPI07_TUIKA_NOROI_003,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Enter the Dark Realm quickly and\n"
"defeat your enemy.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:EPI07_TUIKA_NOROI_004,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou melts into another realm,\n"
"pursuing the source of the curse...\n"
"<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:IKAI_SETUMEI_01,segment:0"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The Dark Realm lies behind the\n"
"world we know. It is a place where\n"
"demons roam freely.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:IKAI_SETUMEI_01,segment:1"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]The border between the realms is\n"
"thin, and demons sometimes enter\n"
"this world by chance.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:IKAI_SETUMEI_01,segment:2"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]Those who know how to look\n"
"can see a demon's hand in most\n"
"of our world's unsolved crimes.\n"
"[23:]<end>"
msgstr ""

#. Herald of Yatagarasu
msgctxt "type:0,entry:IKAI_SETUMEI_01,segment:3"
msgid ""
"<start>[7:0005,00FF][22:0009,00FF,0006,00FF]I am able to use the Ritual of Entry\n"
"to connect our world with the\n"
"Dark Realm.\n"
"[23:]<end>"
msgstr ""

msgctxt "type:0,entry:MAP_SAVE,segment:0"
msgid ""
"<start>[7:0007,00FF]> It's a Dragon Cave. Mysterious\n"
"power is flowing out of it...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MAP_SAVE,segment:1"
msgid ""
"<start>> Here, Raidou can record his\n"
"findings without being disturbed.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MAP_SAVE,segment:2"
msgid ""
"<start>> Save?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:MAP_SAVE_SEL,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:MAP_HEAL,segment:0"
msgid ""
"<start>[7:0007,00FF]> Recover health?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:MAP_HEAL_SEL,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:MAP_HEAL2,segment:0"
msgid ""
"<start>[7:0007,00FF]> [10:0001,00FF] and\n"
"his demons were healed.\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL_FAST,segment:0"
msgid ""
"<start>[7:0007,00FF]*sob* *sob*\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL_FAST,segment:1"
msgid ""
"<start>...Who are you? I've never seen\n"
"you around here...\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL_FAST,segment:2"
msgid ""
"<start>I'm Nakisawame. I heal everyone...\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL_2ND,segment:0"
msgid ""
"<start>[7:0007,00FF]*sob* Welcome.\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL_FULLMOON,segment:0"
msgid ""
"<start>[7:0007,00FF]*sob* Welcome. It's a full moon,\n"
"so...\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL,segment:0"
msgid ""
"<start>[7:0007,00FF]Do you want me to heal you?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:MAP_CHEAL_SEL,segment:0"
msgid "<start>Yes"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL2,segment:0"
msgid ""
"<start>[7:0007,00FF]I'll need [3:0001,00FF] yen... Is that okay?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:MAP_CHEAL2_SEL,segment:0"
msgid "<start>Yes"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL3,segment:0"
msgid ""
"<start>[7:0007,00FF]This may sting a little, but only\n"
"for a moment...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MAP_CHEAL4,segment:0"
msgid ""
"<start>[7:0007,00FF]> [10:0001,00FF] and\n"
"his demons were healed.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:MAP_CHEAL4D,segment:0"
msgid ""
"<start>[7:0007,00FF]> [14:0000,0000] was healed.\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL5,segment:0"
msgid ""
"<start>[7:0007,00FF]*sob* Don't die on me... Please...\n"
"<end>"
msgstr ""

#. Nakisawame
msgctxt "type:0,entry:MAP_CHEAL6,segment:0"
msgid ""
"<start>[7:0007,00FF]*sob* So... You're going to die,\n"
"too...\n"
