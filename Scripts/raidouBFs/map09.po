msgctxt "type:0,entry:m_mes_kikai01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a row of spinning machines.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_nimotu,segment:0"
msgid ""
"<start>[7:0005,00FF]> Packages litter the area.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a doll floating in the air.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01,segment:1"
msgid ""
"<start>> Touch it?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_mato01,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01_0,segment:0"
msgid ""
"<start>[7:0005,00FF]> The doll suddenly flashed brightly!\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to touch the\n"
"doll.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_mato01_2,segment:0"
msgid ""
"<start>[7:0005,00FF]Hang on, [12:0001,00FF].\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_mato01_2,segment:1"
msgid ""
"<start>If you know you're walking into a\n"
"trap, there's no sense in going in\n"
"unprepared.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_mato01_2,segment:2"
msgid ""
"<start>You sure you're ready for this?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01_3,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou reached for the doll.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01_3,segment:1"
msgid ""
"<start>> The doll began to glow, filling\n"
"the room with light...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_mato01_4,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to touch the\n"
"doll.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> The door is sealed.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_02,segment:0"
msgid ""
"<start>[7:0005,00FF]> Break the seal with the [2:0005,00FF]Blood\n"
"Chrysoberyl[2:0001,00FF]?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_kekkai_02,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_02_0,segment:0"
msgid ""
"<start>[7:0005,00FF]> The seal is broken. Raidou felt\n"
"the presence of a strong demon from\n"
"beyond the door.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_02_0,segment:1"
msgid ""
"<start>> Go inside?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_kekkai,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_0,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou opened the door and went\n"
"inside.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to go inside.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_02_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou left.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_keynasi,segment:0"
msgid ""
"<start>[7:0005,00FF]Looks like we need to do something\n"
"about that seal.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_keynasi,segment:1"
msgid ""
"<start>According to that voice, we need\n"
"[2:0004,00FF]three shards to break the seal[2:0001,00FF]\n"
"inside the Matrix Labyrinth.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kekkai_03,segment:0"
msgid ""
"<start>[7:0005,00FF]> The power of the jewel has melted\n"
"the seal away...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_matohaitta,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou found himself in a strange\n"
"world.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_matosyokai,segment:0"
msgid ""
"<start>[7:0005,00FF]Is this that Matrix Labyrinth the\n"
"voice was yammering about?\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_matosyokai,segment:1"
msgid ""
"<start>We might have walked into his trap\n"
"on purpose, but we'll still need to\n"
"stay on our toes.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate01_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> A strange wall is blocking\n"
"the way.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate02_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> The gate cannot be entered from\n"
"this side.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate03_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou was unable to pass through\n"
"the gate.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate04,segment:0"
msgid ""
"<start>[7:0005,00FF]> Unearthly lights are swirling\n"
"around...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate04,segment:1"
msgid ""
"<start>> Touch them?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_gate04,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_mes_gate04_0,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou passed through.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_gate04_1,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to touch them.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti01_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a mysterious switch that\n"
"seems susceptible to fire attacks.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti01_01,segment:1"
msgid ""
"<start>> Destroy it?\n"
"<end>"
msgstr ""

msgctxt "type:1,entry:m_sel_suitti01,segment:0"
msgid "<start>Yes"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti01_0,segment:0"
msgid ""
"<start>[7:0005,00FF]temp message\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti01_0,segment:1"
msgid ""
"<start>temp message\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti01_1,segment:0"
msgid ""
"<start>[7:0005,00FF]temp message\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti02_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a mysterious switch that\n"
"seems susceptible to force attacks.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti02_01,segment:1"
msgid ""
"<start>> Destroy it?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti03_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a mysterious switch that\n"
"seems susceptible to ice attacks.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti03_01,segment:1"
msgid ""
"<start>> Destroy it?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti04_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a mysterious switch that\n"
"seems susceptible to lightning\n"
"attacks.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti04_01,segment:1"
msgid ""
"<start>> Destroy it?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti05_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> It's a mysterious switch that\n"
"seems susceptible to power attacks.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_suitti05_01,segment:1"
msgid ""
"<start>> Destroy it?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_rkeyget,segment:0"
msgid ""
"<start>[7:0005,00FF]> There's something on the ground.\n"
"Pick it up?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_rkeyget02,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained a [2:0005,00FF]Red Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_rkeyget02d,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] obtained a [2:0005,00FF]Red Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_bkeyget,segment:0"
msgid ""
"<start>[7:0005,00FF]> There's something on the ground.\n"
"Pick it up?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_bkeyget02,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained a [2:0005,00FF]Blue Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_bkeyget02d,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] obtained a [2:0005,00FF]Blue Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_wkeyget,segment:0"
msgid ""
"<start>[7:0005,00FF]> There's something on the ground.\n"
"Pick it up?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_wkeyget02,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained a [2:0005,00FF]White Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_wkeyget02d,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] obtained a [2:0005,00FF]White Shard[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_noget,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to pick it up.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_noget_d,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] decided not to pick\n"
"it up.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_noget_d2,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] sensed...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_noget_d2,segment:1"
msgid ""
"<start>> ...that Raidou should pick it\n"
"up, and stopped.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kikannji,segment:0"
msgid ""
"<start>[7:0005,00FF]> Unearthly lights are swirling\n"
"around...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kikannji,segment:1"
msgid ""
"<start>> Touch them?\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kikannji_yes,segment:0"
msgid ""
"<start>[7:0005,00FF]> The area was filled with light...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kikannji_no,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou decided not to touch them.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kikannji_nakama,segment:0"
msgid ""
"<start>[7:0005,00FF]> The demon is interested in the\n"
"lights, but it's also worried about\n"
"Raidou.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki01,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] examined the door.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki02,segment:0"
msgid ""
"<start>[7:0005,00FF]> The door's seal has already been\n"
"broken.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_l01,segment:0"
msgid ""
"<start>[7:0005,00FF]> Something is jamming the door\n"
"shut.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_l01,segment:1"
msgid ""
"<start>> It might open with [2:0004,00FF]power[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_l02,segment:0"
msgid ""
"<start>[7:0005,00FF]> The door seems to open normally.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_l03,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] put all its strength\n"
"into opening the door.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_l04,segment:0"
msgid ""
"<start>[7:0005,00FF]> The object jamming the door came\n"
"loose, and the door opened.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_kansiki_c_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> The seal is holding the door\n"
"tightly shut.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_raidou_open,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou didn't have the strength\n"
"to open the door.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_annai_01,segment:0"
msgid ""
"<start>[7:0005,00FF]Psst, over here. You can get through\n"
"this way.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_annai_02,segment:0"
msgid ""
"<start>[7:0005,00FF]It's a one-way entrance, though.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_annai_03,segment:0"
msgid ""
"<start>[7:0005,00FF]There's some sorta trick to this\n"
"wall...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_01,segment:0"
msgid ""
"<start>[7:0005,00FF]Oh, fiddlesticks. I can't find my\n"
"glasses anywhere.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_01,segment:1"
msgid ""
"<start>Every one of these darn passages\n"
"looks the same to me.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_01,segment:2"
msgid ""
"<start>I just can't see a thing without my\n"
"glasses...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_02,segment:0"
msgid ""
"<start>[7:0005,00FF]Ohh, this is the living end! In all\n"
"my years, I've never seen anything\n"
"so frustrating!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_02,segment:1"
msgid ""
"<start>Everywhere I go, it's walls, walls,\n"
"walls! Who designed this tacky\n"
"labyrinth?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_02,segment:2"
msgid ""
"<start>Honestly! If you ask me, somebody\n"
"needs to be disciplined.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_01,segment:0"
msgid ""
"<start>[7:0005,00FF]Whoa!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_01,segment:1"
msgid ""
"<start>You're a normal joe, right?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_01,segment:2"
msgid ""
"<start>Naw, man, I didn't mean it like that.\n"
"I just saw this weird red guy zip past\n"
"at warp speed.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_01,segment:3"
msgid ""
"<start>That red cape of his was far out,\n"
"though. I'll gank it next time he\n"
"comes around.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_02,segment:0"
msgid ""
"<start>[7:0005,00FF]Whoa! Oh, wait... it's just a black one.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_02,segment:1"
msgid ""
"<start>You just ran by me a little while ago,\n"
"but it was a red you.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_02,segment:2"
msgid ""
"<start>Are you like, the rare black variant?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_03,segment:0"
msgid ""
"<start>[7:0005,00FF]Hey, did you see a far-out red guy?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_03,segment:1"
msgid ""
"<start>I dunno if he's a man or a demon,\n"
"but he looked wicked cool in that\n"
"red cape.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_03_03,segment:2"
msgid ""
"<start>He's made himself scarce, though.\n"
"Is he your bud or something?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_04,segment:0"
msgid ""
"<start>[7:0005,00FF]Hee ho...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_04,segment:1"
msgid ""
"<start>Have you seen-ho my friend,\n"
"hee ho?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_04,segment:2"
msgid ""
"<start>We're playing hee-and seek, ho.\n"
"But, I can't find my friend, ho...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_04,segment:3"
msgid ""
"<start>D-Did they go hee-home without me,\n"
"hee-ho!?\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_04,segment:4"
msgid ""
"<start>I'm lonely, ho...\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_05,segment:0"
msgid ""
"<start>[7:0005,00FF]Hee ho! Hee-and-seek, ho!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_05,segment:1"
msgid ""
"<start>You can't get hee-here unless you\n"
"come from-ho the outside, hee ho.\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_05,segment:2"
msgid ""
"<start>I'm safe hee-here, ho!\n"
"<end>"
msgstr ""

#. Demon
msgctxt "type:0,entry:m_mes_akunpc_05,segment:3"
msgid ""
"<start>But, I'm getting tired, ho...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_01,segment:0"
msgid ""
"<start>[7:0005,00FF]> The three shards began to merge\n"
"together in Raidou's hand...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_01d,segment:0"
msgid ""
"<start>[7:0005,00FF]> The three shards began to merge\n"
"together...\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_02,segment:0"
msgid ""
"<start>[7:0005,00FF]> The three shards coalesced into\n"
"a crimson jewel.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_03,segment:0"
msgid ""
"<start>[7:0005,00FF]> The three shards fused into a key.\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_04,segment:0"
msgid ""
"<start>[7:0005,00FF]> Raidou obtained the [2:0005,00FF]Blood\n"
"Chrysoberyl[2:0001,00FF].\n"
"<end>"
msgstr ""

msgctxt "type:0,entry:m_mes_tckey_get_04d,segment:0"
msgid ""
"<start>[7:0005,00FF]> [14:0000,0000] obtained the [2:0005,00FF]Blood\n"
"Chrysoberyl[2:0001,00FF].\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_tckey_get_05,segment:0"
msgid ""
"<start>[7:0005,00FF]Attaboy. Now, we can break that\n"
"seal.\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_tckey_get_05,segment:1"
msgid ""
"<start>Shake a leg, [12:0001,00FF]! We can't\n"
"let Kiyoshi escape this time!\n"
"<end>"
msgstr ""

#. Gouto
msgctxt "type:0,entry:m_mes_tckey_get_06,segment:0"
msgid ""
"<start>[7:0005,00FF]I thought so. What a pain...\n"
"Kiyoshi's up next, [12:0001,00FF].\n"
"This key should get us through.\n"
"<end>"
msgstr ""

